{
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/.next": true,
    "**/.turbo": true
  },
  "tailwindCSS.experimental.classRegex": [
    ["Classes \\=([^;]*);", "'([^']*)'"],
    ["Classes \\=([^;]*);", "\"([^\"]*)\""],
    ["Classes \\=([^;]*);", "\\`([^\\`]*)\\`"]
  ],
  "tailwindCSS.classFunctions": ["tv"],
  "tailwindCSS.experimental.configFile": "packages/theme/src/premium.css",
  "explorerExclude.backup": {},
  "cSpell.words": [
    "jamsrui",
    "labelledby"
  ]
}
